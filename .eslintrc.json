{
  "extends": "next/core-web-vitals",
  "ignorePatterns": [
    "src/generated/**/*",
    "node_modules/**/*",
    ".next/**/*",
    "out/**/*",
    "dist/**/*"
  ],
  "rules": {
    "@typescript-eslint/no-unused-expressions": "off",
    "@typescript-eslint/no-unused-vars": "warn",
    "@typescript-eslint/no-this-alias": "off",
    "@typescript-eslint/no-require-imports": "off"
  }
} 